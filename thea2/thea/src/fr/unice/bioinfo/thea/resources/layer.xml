<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC '-//NetBeans//DTD Filesystem 1.0//EN' 'http://www.netbeans.org/dtds/filesystem-1_0.dtd'>
<filesystem>
  <!-- =============== BEGIN MENUS ============= -->
  <folder name="Menu">
    <attr name="SystemFileSystem.localizingBundle" stringvalue="fr.unice.bioinfo.thea.ontologyexplorer.Bundle"/>
    <!-- ========== Windows Menu ================ -->
    <folder name="Window">
      <attr
        name="fr-unice-bioinfo-thea-ontologyexplorer-actions-OntologyExplorerAction.instance/org-netbeans-core-execution-ExecutionViewAction.instance" boolvalue="true"/>
      <file name="fr-unice-bioinfo-thea-ontologyexplorer-actions-OntologyExplorerAction.instance"/>
    </folder>
    <!-- ========== END Windows Menu ================ -->
  </folder>
  <!-- ================ END MENUS =============== -->
  <!-- ================ BEGINE WINDOWS2 =============== -->
  <folder name="Windows2">
    <folder name="Modes">
      <file name="editor.wsmode" url="editor.wsmode"/>
      <file name="explorer.wsmode" url="explorer.wsmode"/>
      <folder name="explorer">
        <file name="ontologyexplorer.wstcref" url="ontologyexplorer.wstcref"/>
        <file name="selectioneditor.wstcref" url="selectioneditor.wstcref"/>
      </folder>
    </folder>
    <folder name="Components">
      <file name="ontologyexplorer.settings" url="ontologyexplorer.settings"/>
    </folder>
  </folder>
  <!-- ================ END WINDOWS2 =============== -->
  <!-- ================ SERVICES  =============== -->
  <folder name="Services">
    <folder name="JDBCDrivers">
      <file name="JDBC-ODBC Bridge.xml" url="JDBCODBC.xml"/>
    </folder>
    <folder name="Hidden">
      <folder name="OntologyExplorer">
        <file name="OESettings.settings" url="OESettings.settings"/>
      </folder>
    </folder>
  </folder>
  <!-- ================ SERVICES  =============== -->
</filesystem>
